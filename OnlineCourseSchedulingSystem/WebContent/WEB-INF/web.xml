    <servlet-name>locationUpdate</servlet-name>
    <url-pattern>/locationUpdate</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>courseUpdate</servlet-name>
    <servlet-class>com.ocms.servlet.course.CourseUpdateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>courseUpdate</servlet-name>
    <url-pattern>/courseUpdate</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>courseList</servlet-name>
    <servlet-class>com.ocms.servlet.course.CourseListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>courseList</servlet-name>
    <url-pattern>/CourseListServlet</url-pattern>
  </servlet-mapping>
</web-app>